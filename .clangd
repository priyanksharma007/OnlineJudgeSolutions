CompileFlags:
  Add: [
    "-std=c++20",
    "--gcc-toolchain=/opt/homebrew/opt/gcc",
    "-nostdinc++",
    "-isystem",
    "/opt/homebrew/Cellar/gcc/15.2.0/include/c++/15",
    "-isystem",
    "/opt/homebrew/Cellar/gcc/15.2.0/include/c++/15/aarch64-apple-darwin24",
    "-isystem",
    "/opt/homebrew/Cellar/gcc/15.2.0/include/c++/15/backward",
    "-isystem",
    "/opt/homebrew/include",
    "-isystem",
    "/opt/homebrew/include/c++/15"
  ]
Diagnostics:
  Suppress: ["unresolved-included-file", "constexpr-not-constant", "constexpr-eval"]
